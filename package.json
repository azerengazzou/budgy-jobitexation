{
  "name": "budgy",
  "main": "expo-router/entry",
  "version": "1.1.7",
  "private": true,
  "type": "commonjs",
  "funding": false,
  "overrides": {
    "@react-native-community/cli": "^20.0.2",
    "send": "^0.19.0"
  },
  "scripts": {
    "dev": "expo start",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild",
    "build:android": "expo prebuild --platform android && cd android && gradlew assembleRelease",
    "build:android:debug": "expo prebuild --platform android && cd android && gradlew assembleDebug",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build:production": "npm run bundle:android && cd android && gradlew assembleRelease"
  },
  "dependencies": {
